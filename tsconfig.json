{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "ES6",
        "downlevelIteration": true,
        "esModuleInterop": true,
        "outDir": "dist",
        "noImplicitAny": true,
        "module": "CommonJS",
        "declaration": true
    },
    "include": ["src/**/*.ts"],
    "typedocOptions": {
        "entryPoints": ["src/List.ts"],
        "out": "docs",
        "theme": "minimal"
    }
}